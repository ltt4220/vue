<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <title>zmEditor</title>
    <script src="/static/zmEditor.js"></script>
  </head>
  <body>
    <div id="setpanel">
			<li :is="item.component" :prop="item.prop" v-for="item in items"></li>
		</div>
		<hr />
		<div id="actionpanel">
			<li :is="item.component" :prop="item.prop" v-for="item in items"></li>
		</div>
		<hr />
		<div id="com">
			<button @click="addButton('test')">添加按钮</button>
			<button @click="addLogo('/static/logo.png')">添加LOGO</button>
			<button @click="removeCom()">删除组件</button>
			<hr />
			<li :is="item.component" :prop="item.prop" v-for="item in items"></li>
		</div>
  </body>
</html>
